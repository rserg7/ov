{
    "global": {
        "primitives": {
            "color-primary-50": "#EFF6FF",
            "color-primary-100": "#DBEAFE",
            "color-primary-500": "#3B82F6",
            "color-primary-900": "#1E3A8A",
            "color-surface-0": "#FFFFFF",
            "color-surface-50": "#F9FAFB",
            "color-surface-100": "#F3F4F6",
            "color-surface-500": "#6B7280",
            "color-surface-900": "#111827",
            "color-white": "#FFFFFF",
            "color-black": "#000000",
            "border-radius-none": "0px",
            "border-radius-sm": "4px",
            "border-radius-md": "8px",
            "border-radius-lg": "16px",
            "border-radius-full": "9999px"
        },
        "semantic": {
            "colors": {
                "primary": "{primitives.color-primary-500}",
                "primary-contrast": "{primitives.color-white}",
                "primary-emphasis": "{primitives.color-primary-900}",
                "surface-0": "{primitives.color-surface-0}",
                "surface-50": "{primitives.color-surface-50}",
                "surface-100": "{primitives.color-surface-100}",
                "surface-500": "{primitives.color-surface-500}",
                "surface-900": "{primitives.color-surface-900}",
                "border-surface": "{primitives.color-surface-100}",
                "bg-emphasis": "{primitives.color-surface-900}",
                "bg-highlight": "{primitives.color-surface-50}",
                "bg-highlight-emphasis": "{primitives.color-surface-100}",
                "text-color": "{primitives.color-surface-900}",
                "text-color-emphasis": "{primitives.color-primary-500}",
                "text-muted-color": "{primitives.color-surface-500}",
                "text-muted-color-emphasis": "{primitives.color-surface-900}"
            },
            "borderRadius": {
                "none": "{primitives.border-radius-none}",
                "sm": "{primitives.border-radius-sm}",
                "md": "{primitives.border-radius-md}",
                "lg": "{primitives.border-radius-lg}",
                "full": "{primitives.border-radius-full}"
            }
        },
        "component": {
            "buttons": {
                "button-bg": "{semantic.colors.primary}",
                "button-text": "{semantic.colors.primary-contrast}",
                "button-border": "{semantic.colors.border-surface}",
                "button-border-radius": "{semantic.borderRadius.md}",
                "button-padding": "16px",
                "button-hover-bg": "{semantic.colors.primary-emphasis}",
                "button-hover-text": "{semantic.colors.primary-contrast}",
                "button-active-bg": "{semantic.colors.primary-900}",
                "button-disabled-bg": "{semantic.colors.surface-100}",
                "button-disabled-text": "{semantic.colors.text-muted-color}"
            },
            "inputs": {
                "input-bg": "{semantic.colors.surface-0}",
                "input-text": "{semantic.colors.text-color}",
                "input-border": "{semantic.colors.border-surface}",
                "input-border-radius": "{semantic.borderRadius.sm}",
                "input-padding": "12px",
                "input-focus-border": "{semantic.colors.primary}",
                "input-placeholder-text": "{semantic.colors.text-muted-color}",
                "input-disabled-bg": "{semantic.colors.surface-50}",
                "input-error-border": "{semantic.colors.primary-emphasis}"
            },
            "cards": {
                "card-bg": "{semantic.colors.surface-50}",
                "card-text": "{semantic.colors.text-color}",
                "card-border": "{semantic.colors.border-surface}",
                "card-border-radius": "{semantic.borderRadius.lg}",
                "card-padding": "20px",
                "card-shadow": "0px 4px 6px rgba(0, 0, 0, 0.1)"
            },
            "dropdowns": {
                "dropdown-bg": "{semantic.colors.surface-0}",
                "dropdown-text": "{semantic.colors.text-color}",
                "dropdown-border": "{semantic.colors.border-surface}",
                "dropdown-border-radius": "{semantic.borderRadius.md}",
                "dropdown-padding": "12px",
                "dropdown-hover-bg": "{semantic.colors.bg-highlight}",
                "dropdown-active-bg": "{semantic.colors.primary-50}"
            },
            "modals": {
                "modal-bg": "{semantic.colors.surface-0}",
                "modal-border-radius": "{semantic.borderRadius.md}",
                "modal-padding": "24px",
                "modal-shadow": "0px 10px 15px rgba(0, 0, 0, 0.1)",
                "modal-overlay-bg": "rgba(0, 0, 0, 0.5)"
            },
            "tables": {
                "table-header-bg": "{semantic.colors.surface-100}",
                "table-row-bg": "{semantic.colors.surface-0}",
                "table-row-hover-bg": "{semantic.colors.bg-highlight}",
                "table-border": "{semantic.colors.border-surface}",
                "table-text": "{semantic.colors.text-color}"
            },
            "tabs": {
                "tab-bg": "{semantic.colors.surface-0}",
                "tab-text": "{semantic.colors.text-muted-color}",
                "tab-border": "{semantic.colors.border-surface}",
                "tab-active-bg": "{semantic.colors.primary-50}",
                "tab-active-text": "{semantic.colors.primary-emphasis}",
                "tab-hover-bg": "{semantic.colors.bg-highlight}"
            },
            "alerts": {
                "alert-info-bg": "{semantic.colors.primary-50}",
                "alert-info-text": "{semantic.colors.primary-900}",
                "alert-success-bg": "#D1FAE5",
                "alert-success-text": "#065F46",
                "alert-warning-bg": "#FEF3C7",
                "alert-warning-text": "#92400E",
                "alert-error-bg": "#FEE2E2",
                "alert-error-text": "#991B1B"
            },
            "progress": {
                "progress-bg": "{semantic.colors.surface-100}",
                "progress-fill": "{semantic.colors.primary}",
                "progress-label-text": "{semantic.colors.text-color}"
            },
            "badges": {
                "badge-bg": "{semantic.colors.primary}",
                "badge-text": "{semantic.colors.primary-contrast}",
                "badge-border-radius": "{semantic.borderRadius.full}"
            }
        }
    }
}